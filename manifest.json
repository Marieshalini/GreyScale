
{
  "manifest_version": 3,
  "name": "ColorVision Enhancer",
  "version": "1.0",
  "description": "Enhances website colors for color-blind users ensuring WCAG contrast compliance.",
  "permissions": ["storage", "activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "icons/icon128.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}


